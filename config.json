    "dns": {
        "servers": [
            {
                "tag": "remote-dns",
                "address": "https://dns.google/dns-query",
                "address_resolver": "remote-resolver-dns",
                "address_strategy": "ipv4_only",
                "detour": "Proxy"
            },
            {
                "tag": "local-dns",
                "address": "223.5.5.5",
                "detour": "direct"
            },
            {
                "tag": "remote-resolver-dns",
                "address": "8.8.8.8",
                "detour": "Proxy"
            },
            {
                "tag": "block",
                "address": "rcode://success"
            }
        ],
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": false,
        "rules": [
            {
                "outbound": "any",
                "disable_cache": true,
                "server": "local-dns"
            },
            {
                "rule_set": [
                    "GEOSITE-CN"
                ],
                "server": "local-dns"
            },
            {
                "rule_set": [
                    "GEOLOCATION-!CN"
                ],
                "server": "remote-dns"
            },
			{
                "rule_set": [
                    "CATEGORY-ADS"
                ],
                "server": "block"
            },
            {
                "clash_mode": "direct",
                "server": "local-dns"
            },
            {
                "clash_mode": "global",
                "server": "remote-dns"
            }
        ],
        "fakeip": {
            "enabled": false,
            "inet4_range": "198.18.0.1/16"
        },
        "final": "local-dns",
        "strategy": "ipv4_only"
    },
    "ntp": {
        "enabled": true,
        "server": "time.apple.com",
        "server_port": 123,
        "interval": "30m",
        "detour": "direct"
    },
    "inbounds": [
        {
            "type": "tun",
            "address":"172.19.0.1/30",
            "mtu": 9000,
            "auto_route": true,
			"auto_redirect": true,
			"strict_route": true,
            "sniff": true,
            "stack": "system",
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 2080
                }
            }
        },
        {
            "type": "mixed",
            "listen": "127.0.0.1",
            "listen_port": 2080,
            "sniff": true,
            "users": [

            ]
        }
    ],
    "route": {
        "rule_set": [
            {
                "type": "remote",
                "format": "binary",
                "download_detour": "Proxy",
                "tag": "GEOIP-CN",
                "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs"
            },
            {
                "type": "remote",
                "format": "binary",
                "download_detour": "Proxy",
                "tag": "GEOSITE-CN",
                "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs"
            },
			{
                "type": "remote",
                "format": "binary",
                "download_detour": "Proxy",
                "tag": "GEOLOCATION-!CN",
                "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/geolocation-!cn.srs"
            },
			{
                "type": "remote",
                "format": "binary",
                "download_detour": "Proxy",
                "tag": "CATEGORY-ADS",
                "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs"
            }
        ],
        "rules": [
            {
                "type": "logical",
                "mode": "or",
                "rules": [
                    {
                        "port": 53
                    },
                    {
                        "protocol": "dns"
                    }
                ],
                "outbound": "dns-out"
            },
			{
                "network": "udp",
                "port": 443,
                "outbound": "block"
            },
            {
                "clash_mode": "direct",
                "outbound": "direct"
            },
            {
                "clash_mode": "global",
                "outbound": "Proxy"
            },
            {
                "rule_set": [
                    "GEOIP-CN",
					"GEOSITE-CN"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "GEOLOCATION-!CN"
                ],
                "outbound": "Proxy"
            },
			            {
                "rule_set": [
                    "CATEGORY-ADS"
                ],
                "outbound": "block"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            }
        ],
        "auto_detect_interface": true,
        "final": "Proxy"
    }
}