    "outbounds": [
        {
            "tag": "Proxy",
            "outbounds": [
                "jp1",
                "direct",
                "us"
            ],
            "default": "us",
            "interrupt_exist_connections": true,
            "type": "selector"
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "dns",
            "tag": "dns-out"
