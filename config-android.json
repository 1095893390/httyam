            },
            {
                "type": "logical",
                "mode": "and",
                "rules": [
                    {
                        "port": 443
                    },
                    {
                        "network": "udp"
                    }
                ],
                "outbound": "block"
            },
            {
                "domain_keyword": [
                    "adobe",
                    "Adobe"
                ],
                "outbound": "block"
            },
            {
                "clash_mode": "direct",
                "outbound": "direct"
            },
            {
                "clash_mode": "global",
                "outbound": "Proxy"
            },
            {
                "rule_set": [
                    "GEOIP-CN",
                    "GEOSITE-CN"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "GEOLOCATION-!CN"
                ],
                "outbound": "Proxy"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            }
        ],
        "auto_detect_interface": true,
        "final": "Proxy"
