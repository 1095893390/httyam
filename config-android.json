            },
            {
                "tag": "block",
                "address": "rcode://success"
            }
        ],
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": false,
        "rules": [
            {
                "outbound": "any",
                "disable_cache": true,
                "server": "local-dns"
            },
            {
                "domain_keyword": [
                    "adobe",
                    "Adobe"
                ],
                "server": "block"
            },
            {
